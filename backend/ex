function db() {
var mysql      = require('mysql');
var connection = mysql.createConnection({ 
  user     : 'root',
  password : '',
  database	: 'test'  
});

connection.connect(function(err) {
	console.log('connected')
});

connection.query('select * from user', function(err, result) {
  console.log(result)
});

connection.end();
}